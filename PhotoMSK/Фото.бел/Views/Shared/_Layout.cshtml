<body>
    <nav class="nav">
        <div class="beta"></div>
        <section class="container container--center" data-ng-if="!showTopMenu">
            <div class="grid grid--small" data-ng-controller="userMenuCtrl">
                <a href="/" class="nav__logo col--large-1-10 col--medium-1-10">
                    <img src="@Url.Content(" Content/images/logo/logo-white-yellow.svg ")" width="90" alt="Сайт для фотографов" />
                </a>
                <ul class="nav__list nav__list__menu col--large-5-10 col--medium-6-10">
                    <li><a href="/photostudio">Cтудии</a></li>
                    <li><a href="/photographer">Фотографы</a></li>
                    <li class="repair repair--text"><a href="/phototechnics">Техника</a></li>
                    <li class="repair repair--text"><a href="/">События</a></li>
                    <li class="repair repair--text"><a href="/photomonth">Фотографии</a></li>
                </ul>

                <ul class="nav__list col--large-4-10 col--medium-3-10 text--right" data-ng-cloak>
                    <li data-ng-if="!authService.authentication.isAuth">
                        <a href="/login">
                            <span>Вход</span>
                            <span data-ng-model="userInformation.isAuthenticated"></span>
                        </a>
                    </li>

                    <li data-ng-if="authService.authentication.isAuth && userEventsData.length > 0">
                        <a data-msk-href popover-placement="bottom" uib-popover-template="'Scripts/admin/components/shared/Notification/notification-booking.html'">
                            <span class="icon-ticket nav__account--notifacation">
                                <i>{{userEventsData.length}}</i>
                            </span>
                        </a>
                    </li>

                    <li class="nav__list__city">
                        <a data-msk-href data-msk-href popover-placement="bottom" uib-popover-template="'Scripts/admin/components/shared/Notification/search-location.html'">
                            <span>Минск</span>
                        </a>
                    </li>

                    <li class="nav__account" data-ng-if="authService.authentication.isAuth" uib-dropdown dropdown-append-to-body>
                        <a data-msk-href uib-dropdown-toggle>
                            <img data-ng-src="{{userInformation.userPhoto}}" alt="{{userInformation.lastName}}" data-ng-if="userInformation.userPhoto != null" data-ng-cloak />
                            <div class="avatar avatar--empty float--left margin--full-mini" data-ng-if="userInformation.userPhoto == null"></div>
                            <span class="hidden--medium" data-ng-bind-template="{{userInformation.firstName}} {{userInformation.lastName}}" data-ng-cloak></span>
                        </a>
                        <ul uib-dropdown-menu role="menu" aria-labelledby="btn-append-to-body">
                            <li role="menuitem"><a href="/admin/">Мой кабинет</a></li>
                            <li class="divider"></li>
                            <li role="menuitem"><a data-msk-href data-ng-click="authService.logOut()">Выйти</a></li>
                        </ul>
                    </li>
                </ul>

            </div>
        </section>
    </nav>

