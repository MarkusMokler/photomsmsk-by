}

@section ToolbarR
{
    @Html.Partial("~/Themes/Photorent/photoprokat.by/Views/Toolbar-R/Search.cshtml")
    @Html.Partial("~/Themes/Photorent/photoprokat.by/Views/Toolbar-R/Cart.cshtml")
}

@section ToolbarL
