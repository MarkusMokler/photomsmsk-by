@using Newtonsoft.Json.Linq
@using PhotoMSK.Extensiolns
@model PhotoMSK.Areas.Default.ViewData.Catalog.VitrineViewData

@section MainTopMenu
{
    @Html.Partial("~/Themes/Photoshop/ftt.by/Views/MainTopMenu/MainTopMenu.cshtml")
}

@section TopA
{
    @Html.Partial("~/Themes/Photoshop/ftt.by/Views/Top-A/Maps.cshtml")
}

@section ToolbarR
{
    @Html.Partial("~/Themes/Photoshop/ftt.by/Views/Toolbar-R/Phones.cshtml")
}

@section ToolbarL
{
    @Html.Partial("~/Themes/Photoshop/ftt.by/Views/Toolbar-L/Logo.cshtml")
