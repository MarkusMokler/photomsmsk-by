@section scripts
{
    <script>
    $(function() {
        App.owner = '@Model.RouteID';
        var item = @Model.ToJson();
        item.technics =  @Model.Technics.ToJson();

        var val = new App.Photoshop.Photoshop({model:item});
        val.fetchSuccess(@Model.Technics.ToJson());
    })
    </script>
}
