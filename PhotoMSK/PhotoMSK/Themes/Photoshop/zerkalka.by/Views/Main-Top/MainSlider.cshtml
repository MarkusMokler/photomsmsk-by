@using PhotoMSK.Extensiolns
@model PhotoMSK.Areas.Default.ViewData.WhiteLabelViewData

<div class="uk-grid uk-grid-small">
    <div class="uk-width-3-10">
        <div class="uk-panel uk-panel-box uk-panel-box-dark">
            <span class="uk-h3">
                <a href="@Html.MakeUrl(Model.Route,"catalog")" class="uk-link-reset"><span class="uk-image-marker"></span> КАТАЛОГ</a>
            </span>
        </div>
        <div class="uk-panel uk-panel-box uk-panel-left-category">
            <ul class="uk-list uk-list-line">
                @for (int i = 0; i < 11; i++)
                {
                    <li><a href="#">Проверка</a></li>
                }
            </ul>
        </div>
    </div>
    <div class="uk-width-7-10">
        <div class="uk-slidenav-position" data-uk-slideshow="{autoplay:true}">
            <ul class="uk-slideshow">
                <li class="uk-active">
                    <div class="uk-cover-background uk-position-cover" style="background-image: url(@Url.Content("~/Themes/Photoshop/ftt.by/Content/images/room/fotoroom-1.jpg"));"></div>
                    <img src="@Url.Content("~/Themes/Photoshop/ftt.by/Content/images/room/fotoroom-1.jpg")" alt="">
