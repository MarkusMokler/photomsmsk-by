@model PhotoMSK.Areas.Default.ViewData.Place.IndexViewData

@{
    ViewBag.Title = "Index";
}

<div class="uk-container uk-container-center pad-top">
    <div class="uk-grid">
        <div class="uk-width-1-1 uk-text-center">
            <h1 class="blue-color uk-margin-remove">Совсем скоро</h1>
            <small class="uk-text-muted">Мы работаем над созданием данного раздела...</small>
        </div>
        <div class="uk-width-1-1">
            <ul class="uk-pins position-relative">
                <li class="one"><span class="pin"><img src="@Url.Content("../Content/images/pins/pin_01.png")" alt="icon"></span><span class="shade"><img src="@Url.Content("../Content/images/pins/shadow.png")" alt="shadow"></span><span class="bubble">Будет круто!</span></li>
                <li class="two"><span class="pin"><img src="@Url.Content("../Content/images/pins/pin_04.png")" alt="icon"></span><span class="shade"><img src="@Url.Content("../Content/images/pins/shadow.png")" alt="shadow"></span></li>
                <li class="three"><span class="pin"><img src="@Url.Content("../Content/images/pins/pin_03.png")" alt="icon"></span><span class="shade"><img src="@Url.Content("../Content/images/pins/shadow.png")" alt="shadow"></span></li>
                <li class="four"><span class="pin"><img src="@Url.Content("../Content/images/pins/pin_02.png")" alt="icon"></span><span class="shade"><img src="@Url.Content("../Content/images/pins/shadow.png")" alt="shadow"></span></li>
                <li class="fifth"><img src="@Url.Content("../Content/images/pins/pin_ghost.png")" alt="icon"></li>
            </ul>
        </div>
    </div>
</div>