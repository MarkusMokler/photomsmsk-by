@* Аватар *@
<div class="uk-width-1-1">
    <div class="uk-section">
        <span class="uk-section__counter">3</span>
        <span class="uk-section__title">Время проведения</span>
    </div>

    <div class="uk-grid">
        <div class="uk-width-2-10">
            <label class="label-ms">Дата <i class="icon-help2" data-uk-tooltip title="Введите первую дату проведения мастер-класса"></i></label>
            <input class="form-control input-ms uk-width-large-1-1 uk-width-small-1-1" id="Shortcut" name="Shortcut" type="text" value="">
            @*@Html.TextBoxFor(model => model.StartDay, new { @class = "input-ms uk-width-large-1-1 uk-width-small-1-1", data_uk_datepicker = "{format:'YYYY-MM-DD'}" })
                @Html.ValidationMessageFor(model => model.StartDay)*@
        </div>
        <div class="uk-width-2-10">
            <label class="label-ms">Дней <i class="icon-help2" data-uk-tooltip title="Введите количество дней проведения мастер класса"></i></label>
            <input class="form-control input-ms uk-width-large-1-1 uk-width-small-1-1" id="Shortcut" name="Shortcut" type="text" value="">
            @*@Html.TextBoxFor(model => model.Days, new { @class = "input-ms uk-width-large-1-1 uk-width-small-1-1" })
                @Html.ValidationMessageFor(model => model.Days)*@
        </div>
        <div class="uk-width-2-10">
            <label class="label-ms">Начало<i class="icon-help2" data-uk-tooltip title="Введите время начала мастер-класса"></i></label>
            <input class="form-control input-ms uk-width-large-1-1 uk-width-small-1-1" id="Shortcut" name="Shortcut" type="text" value="">
            @*@Html.TextBoxFor(model => model.Start, new { @class = "input-ms uk-width-large-1-1 uk-width-small-1-1", data_uk_timepicker = "{format:'24h'}" })
                @Html.ValidationMessageFor(model => model.Start)*@
        </div>
        <div class="uk-width-2-10">
            <label class="label-ms">Окончание <i class="icon-help2" data-uk-tooltip title="Введите время окончания мастер-класса"></i></label>
            <input class="form-control input-ms uk-width-large-1-1 uk-width-small-1-1" id="Shortcut" name="Shortcut" type="text" value="">
            @*@Html.TextBoxFor(model => model.End, new { @class = "input-ms uk-width-large-1-1 uk-width-small-1-1", data_uk_timepicker = "{format:'24h'}" })
                @Html.ValidationMessageFor(model => model.End)*@
        </div>
        <div class="uk-width-2-10">
            <label class="label-ms">Человек в группе <i class="icon-help2" data-uk-tooltip title="Введите максимальное количество человек мастер-класса"></i></label>
            <input class="form-control input-ms uk-width-large-1-1 uk-width-small-1-1" id="Shortcut" name="Shortcut" type="text" value="">
            @*@Html.TextBoxFor(model => model.Listeners, new { @class = "input-ms uk-width-large-1-1 uk-width-small-1-1" })
                @Html.ValidationMessageFor(model => model.Listeners)*@
        </div>
    </div>
</div>