    </div>

    <div class="uk-grid">
        <div class="uk-width-2-10 uk-push-2-10 uk-text-center">
            <div id="role-avatar" class="uk-margin-bottom">
                <img src="http://photomsk.by/Content/images/no-avatar.png" class="uk-border-circle uk-text-center">
            </div>
            <a href="#" id="role-change-avatar" class="uk-link-muted uk-button uk-button-link uk-button--arround-dotted">Загрузить</a>
        </div>
        <div class="uk-width-4-10 uk-push-2-10">
            <div class="uk-grid" data-uk-grid-margin>
                <div class="uk-width-1-1">
                    <label class="label-ms">Фамилия</label>
                    <input class="form-control input-ms uk-width-large-1-1 uk-width-small-1-1" id="Shortcut" name="Shortcut" type="text" value="">
                    @*@Html.TextBoxFor(model => model.Title, new { @class = "input-ms uk-width-large-1-1 uk-width-small-1-1" })
                        @Html.ValidationMessageFor(model => model.Title)*@
                </div>
                <div class="uk-width-1-1">
                    <label class="label-ms">Имя</label>
                    <input class="form-control input-ms uk-width-large-1-1 uk-width-small-1-1" id="Shortcut" name="Shortcut" type="text" value="">
                    @*@Html.TextBoxFor(model => model.Title, new { @class = "input-ms uk-width-large-1-1 uk-width-small-1-1" })
                        @Html.ValidationMessageFor(model => model.Title)*@
                </div>
            </div>
        </div>
    </div>
